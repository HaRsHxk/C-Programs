#include <stdio.h>

int main() {
    int year;
     printf("HARSH KUMAR\n");
     printf("1/22/FET/BCS/133\n");
     printf("SECTION-D1\n\n");

    printf("Enter a year: ");
    scanf("%d", &year);

    if ((year % 4 == 0 && year % 100 != 0) || year % 400 == 0) {
        printf("%d is a leap year.", year);
    } else {
        printf("%d is not a leap year.", year);
    }

    return 0;
}
