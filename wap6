#include <stdio.h>

int main() {
    char c;
     printf("HARSH KUMAR\n");
     printf("1/22/FET/BCS/133\n");
     printf("SECTION-D1\n\n");

    printf("Enter a character: ");
    scanf("%c", &c);

    if ((c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z')) {
        printf("%c is an alphabet.\n", c);
    } else if (c >= '0' && c <= '9') {
        printf("%c is a digit.\n", c);
    } else {
        printf("%c is a special character.\n", c);
    }

    return 0;
}
