#include <stdio.h>

int main() {
    char c;
     printf("HARSH KUMAR\n");
     printf("1/22/FET/BCS/133\n");
     printf("SECTION-D1\n\n");

    printf("Enter a character: ");
    scanf("%c", &c);

    if ((c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z'))
        printf("%c is an alphabet.", c);
    else
        printf("%c is not an alphabet.", c);

    return 0;
}
