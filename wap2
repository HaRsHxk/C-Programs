#include <stdio.h>

int main() {
    int num;
     printf("HARSH KUMAR\n");
     printf("1/22/FET/BCS/133\n");
     printf("SECTION-D1\n\n");

    printf("Enter an integer: ");
    scanf("%d", &num);
    
    if(num % 2 == 0) {
        printf("%d is even.", num);
    } else {
        printf("%d is odd.", num);
    }
    
    return 0;
}
